 int Counter;
 void EmptySpotChecker(JButton Butt1,JButton Butt2)//create function
 {
     String ShuffleNumber = Butt2.getText();//move button
     if(ShuffleNumber.equals(""))
     {
         Butt2.setText(Butt1.getText());
         Butt1.setText("");
     }
 }

